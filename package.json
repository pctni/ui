{
	"name": "pctni",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "./build.sh dev && vite dev",
		"dev:clean": "./build.sh --clean && vite dev",
		"dev:safe": "npm run deps:check && npm run dev", 
		"build": "./build.sh && vite build",
		"build:prod": "./build.sh --clean && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"deps:check": "npm list --depth=0 || npm install",
		"deps:clean": "rm -rf node_modules package-lock.json && npm install",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "prettier --write . && eslint . --fix",
		"test:e2e": "playwright test",
		"test": "npm run test:e2e"
	},
	"dependencies": {
		"@svelte-maplibre-gl/pmtiles": "^1.0.1",
		"svelte-maplibre-gl": "^1.0.1"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@fontsource/fira-mono": "^5.0.0",
		"@neoconfetti/svelte": "^2.0.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-netlify": "^5.0.0",
		"@sveltejs/kit": "^2.27.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.37.3",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.6"
	}
}
